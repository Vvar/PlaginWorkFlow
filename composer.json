{
    "name": "git@github.com:Vvar/plugin-work-flow.git",
    "description": "state procedure 223",
    "type": "library",
    "require": {
        "php": "~5.4.0",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.0",
        "friendsofphp/php-cs-fixer": "^2.1.0"
    },
    "autoload": {
        "psr-4": {
            "ETS\\PluginWorkFlow\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "scripts": {
        "cs-check": "php-cs-fixer -v fix --config=.php_cs  --dry-run ",
        "cs-fix": "php-cs-fixer -v fix --config=.php_cs --diff"
    }
}
